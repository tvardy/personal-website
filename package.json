{
  "name": "personal-website",
  "description": "personal-website",
  "version": "0.1.1",
  "main": "app.js",
  "scripts": {
    "dev": "sapper dev",
    "build": "sapper build --legacy",
    "clean": "rm -rf __sapper__ && rm -rf src/node_modules && rm -rf .sass-cache",
    "export": "sapper export --legacy",
    "start": "node .",
    "check": "svelte-check && prettier --write ./src/*",
    "test": "echo \"no tests yet provided\" && exit 0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged",
      "pre-push": "npm run check"
    }
  },
  "dependencies": {
    "aguid": "2.0.0",
    "compression": "1.7.4",
    "dotenv": "8.2.0",
    "express-session": "1.17.2",
    "fast-glob": "3.2.5",
    "fast-memoize": "2.5.2",
    "feed": "4.2.2",
    "front-matter": "4.0.2",
    "got": "11.8.2",
    "helmet": "4.6.0",
    "highlight.js": "10.7.2",
    "lodash.filter": "4.6.0",
    "lodash.find": "4.6.0",
    "luxon": "1.27.0",
    "marked": "2.0.5",
    "memorystore": "1.6.6",
    "morgan": "1.10.0",
    "pify": "5.0.0",
    "polka": "next",
    "remove-markdown": "0.3.0",
    "sirv": "1.0.12",
    "sitemap": "6.4.0"
  },
  "devDependencies": {
    "@babel/core": "7.14.3",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/plugin-transform-runtime": "7.14.3",
    "@babel/preset-env": "7.14.2",
    "@babel/runtime": "7.14.0",
    "@rollup/plugin-babel": "5.3.0",
    "@rollup/plugin-commonjs": "17.1.0",
    "@rollup/plugin-node-resolve": "11.2.0",
    "@rollup/plugin-replace": "2.4.2",
    "@rollup/plugin-url": "6.0.0",
    "@types/marked": "1.2.2",
    "autoprefixer": "9.8.6",
    "cz-conventional-changelog": "3.3.0",
    "husky": "4.3.8",
    "node-sass": "4.14.1",
    "npm-run-all": "4.1.5",
    "prettier": "2.3.0",
    "prettier-plugin-svelte": "2.3.0",
    "pretty-quick": "3.1.0",
    "rollup": "2.50.1",
    "rollup-plugin-node-builtins": "2.1.2",
    "rollup-plugin-svelte": "7.1.0",
    "rollup-plugin-terser": "7.0.2",
    "sapper": "0.29.1",
    "svelte": "3.38.2",
    "svelte-check": "1.6.0",
    "svelte-preprocess": "4.7.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "engines": {
    "node": "12.x",
    "npm": "7.x"
  }
}
