{
  "name": "personal-website",
  "description": "personal-website",
  "version": "0.1.0",
  "main": "app.js",
  "scripts": {
    "dev": "sapper dev",
    "build": "sapper build",
    "clean": "rm -rf __sapper__",
    "export": "sapper export",
    "start": "node .",
    "#cy:run": "cypress run",
    "#cy:open": "cypress open",
    "check": "svelte-check && prettier --write ./src/*",
